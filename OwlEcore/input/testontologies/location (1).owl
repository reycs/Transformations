<?xml version="1.0" encoding="UTF-8"?><rdf:RDF xmlns="http://www.w3.org/2007/uwa/context/location.owl#" xmlns:assert="http://www.owl-ontologies.com/assert.owl#" xmlns:common="http://www.w3.org/2007/uwa/context/common.owl#" xmlns:dc="http://purl.org/dc/terms/" xmlns:dcn="http://www.w3.org/2007/uwa/context/deliverycontext.owl#" xmlns:ex="http://www.example.org/2007/uwa/context/examples.owl#" xmlns:hard="http://www.w3.org/2007/uwa/context/hardware.owl#" xmlns:java="http://www.w3.org/2007/uwa/context/java.owl#" xmlns:loc="http://www.w3.org/2007/uwa/context/location.owl#" xmlns:net="http://www.w3.org/2007/uwa/context/network.owl#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:push="http://www.w3.org/2007/uwa/context/push.owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:soft="http://www.w3.org/2007/uwa/context/software.owl#" xmlns:web="http://www.w3.org/2007/uwa/context/web.owl#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xml:base="http://www.w3.org/2007/uwa/context/location.owl">
  <common:TermGroup rdf:about="#TermGroup_Location">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This group of terms is aimed at defining a set of classes, properties and instances that have to do with Geographic Location</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location</rdfs:label>
        <common:termGroupPrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string">loc</common:termGroupPrefix>
        <common:normativeStatus>
          <common:NormativeStatus rdf:about="http://www.w3.org/2007/uwa/context/common.owl#Utility"/>
        </common:normativeStatus>
      </common:TermGroup>
  <owl:Class rdf:about="#SatelliteLocMethod">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The location method uses satellites to assist in obtaining the location</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Satellite  Method</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#LocationMethod"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#CellIdLocMethod">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location method Cell-ID for cellular (in GSM, this is the same as CGI, Cell Global Identity)</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cell Id Method</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#LocationMethod"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#AngleOfArrivalLocMethod">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Angle Of Arrival Method</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location method Angle of Arrival for cellular / terrestrial RF system</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#LocationMethod"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#GeoCoordinates">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Context_LocationEntity"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#geoReferenceSystem"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#geoReferenceSystem"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#GeoCoordinateRS"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents geographical coordinates that allow to locate precisely a point on the Earth surface</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geographical Coordinates</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#NetworkBasedLocMethod">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location method is of type network based. This means that the final location result is calculated in the network</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Network Based Method</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#LocationMethod"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#GeoCoordinateRS">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geographical Coordinate Reference System</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#id"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#name"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Context_LocationEntity"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a geographical coordinate system</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#LocationMethod">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Context_LocationEntity"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#name"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">It represents an specific mechanism used to calculate the location of a thing</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location Method</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#WGS84Coordinates">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents WGS84 coordinates</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WGS84 Coordinates</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#datum"/>
        </owl:onProperty>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WGS84</owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#longitude"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#latitude"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#GeoCoordinates"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue>
          <loc:GeoCoordinateRS rdf:about="#GeoCoordinateRS_WGS84">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WGS 84</rdfs:label>
            <common:normativeStatus rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Normative"/>
            <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The WGS 84 geographic coordinates reference system</rdfs:comment>
            <common:id rdf:datatype="http://www.w3.org/2001/XMLSchema#string">__GeoCordinateRS_WGS84</common:id>
            <common:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WGS84</common:name>
          </loc:GeoCoordinateRS>
        </owl:hasValue>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#geoReferenceSystem"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#TerminalBasedLocMethod">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location method is of type terminal based. This means that the final location result is calculated in the terminal</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Terminal Based Method</rdfs:label>
    <rdfs:subClassOf rdf:resource="#LocationMethod"/>
  </owl:Class>
  <owl:Class rdf:about="#ShortRangeLocMethod">
    <rdfs:subClassOf rdf:resource="#LocationMethod"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Short Range Method</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location method Short-range positioning system (for example, Bluetooth LP)</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#TimeDifferenceLocMethod">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location method Time Difference for cellular / terrestrial RF system</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#LocationMethod"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Time Difference Method</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#UTMCoordinates">
    <common:normativeStatus rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Informative"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#latitudeZone"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">UTM Coordinates</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#longitudeZone"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#datum"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents UTM coordinates</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#GeoCoordinates"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#northing"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#easting"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#geoReferenceSystem"/>
        </owl:onProperty>
        <owl:hasValue>
          <loc:GeoCoordinateRS rdf:about="#GeoCoordinateRS_UTM">
            <common:id rdf:datatype="http://www.w3.org/2001/XMLSchema#string">__GeocordinateRS_UTM</common:id>
            <common:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">UTM</common:name>
            <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Universal Traversal Marcator coordinate reference system</rdfs:comment>
            <common:normativeStatus rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Utility"/>
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">UTM</rdfs:label>
          </loc:GeoCoordinateRS>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Position">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a position, which it is composed by the coordinates of a point on the Earth surface, together with the speed, course, and the corresponding accuracies and timestamp</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#coordinates"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#GeoCoordinates"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#coordinates"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#iso6709String"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:unionOf>
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Context_LocationEntity"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Position</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#TimeOfArrivalLocMethod">
    <rdfs:subClassOf rdf:resource="#LocationMethod"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location method Time of Arrival (TOA) for cellular / terrestrial RF system</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Time of Arrival Method</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Location">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Position"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#position"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Context_LocationEntity"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents all the information about a location</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#LocationProvider">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#GeoCoordinateRS"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#supportedGeoCoordinateRS"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Context_LocationEntity"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#GeoCoordinateRS"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#supportedGeoCoordinateRS"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#LocationMethod"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#locationMethod"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#LocationMethod"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#locationMethod"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location Provider</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Represents an entity capable of providing location information</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#UnassistedLocMethod">
    <rdfs:subClassOf rdf:resource="#LocationMethod"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unassisted Method</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location method is unassisted it does not need other party in order to obtain the location</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#AssistedLocMethod">
    <rdfs:subClassOf rdf:resource="#LocationMethod"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Assisted Method</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location method is assisted by the other party (Terminal assisted for Network based, Network assisted for terminal based)</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Context_LocationEntity">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class reprensents the set of all delivery context entities that has to do with location</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Context Location Entity</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl#Context_Entity"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#geoReferenceSystem">
    <rdfs:range rdf:resource="#GeoCoordinateRS"/>
    <rdfs:domain rdf:resource="#GeoCoordinates"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents the reference system in which are expressed the geographical coordinates</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georeference System</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#supportedLocationProviders">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property is a list of the Location Providers supported by a Device or a Network</rdfs:comment>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#supports"/>
    </rdfs:subPropertyOf>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl#Device"/>
          <owl:Class rdf:about="http://www.w3.org/2007/uwa/context/network.owl#Network"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#LocationProvider"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Supported Location Providers</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#supportedGeoCoordinateRS">
    <rdfs:domain rdf:resource="#LocationProvider"/>
    <rdfs:range rdf:resource="#GeoCoordinateRS"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indicates what are goegraphical coordinate systems supported by the location provider</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Geographical Coordinate Systems</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#supports"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#availableLocationProviders">
    <rdfs:subPropertyOf rdf:resource="#supportedLocationProviders"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#available"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://www.w3.org/2007/uwa/context/network.owl#Network"/>
          <owl:Class rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl#Device"/>
          <owl:Class rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl#DeliveryContext"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#LocationProvider"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Available Location Providers</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents the LocationProviders that are currently available in a network, device or the Delivery Context itself as whole</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#typicalResponseTime">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Typical Response Time</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents the typical response time in seconds of a location provider from the instant in which the location is requested to the moment in which the location is returned</rdfs:comment>
    <rdfs:domain rdf:resource="#LocationProvider"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:about="#easting">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The easting (x) component of the UTM coordinates measured in metres</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Easting</rdfs:label>
    <common:normativeStatus rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Informative"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#UTMCoordinates"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#timestamp">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A timestamp that indicates when it was obtained the position in question</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Location Timestamp</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:domain rdf:resource="#Position"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#longitude">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The longitude as defined by the WGS84 coordinate system</rdfs:comment>
