          <owl:ObjectProperty rdf:about="#part"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="http://www.loa-cnr.it/ontologies/DnS#situation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
