        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DnS#temporary-component"/>
    <rdfs:domain rdf:resource="http://www.loa-cnr.it/ontologies/DnS#description"/>
