  <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DnS#realized-by">
    <rdfs:range rdf:resource="#particular"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DnS#references"/>
