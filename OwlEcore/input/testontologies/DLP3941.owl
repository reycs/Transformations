          <owl:ObjectProperty rdf:ID="has-quality"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.loa-cnr.it/ontologies/DnS#regulation">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.loa-cnr.it/ontologies/DnS#social-description"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
