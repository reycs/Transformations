        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="q-location"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="inherent-in"/>
        </owl:onProperty>
