          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#proper-part"/>
            </owl:onProperty>
            <owl:allValuesFrom>
              <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
