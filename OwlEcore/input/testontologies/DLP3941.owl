                  <owl:Class rdf:about="http://www.loa-cnr.it/ontologies/DnS#parameter"/>
                  <owl:Restriction>
                    <owl:someValuesFrom rdf:resource="#time-interval"/>
                    <owl:onProperty>
                      <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DnS#valued-by"/>
