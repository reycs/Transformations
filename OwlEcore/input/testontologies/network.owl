<?xml version="1.0" encoding="UTF-8"?><rdf:RDF xmlns="http://www.w3.org/2007/uwa/context/network.owl#" xmlns:assert="http://www.owl-ontologies.com/assert.owl#" xmlns:common="http://www.w3.org/2007/uwa/context/common.owl#" xmlns:dc="http://purl.org/dc/terms/" xmlns:dcn="http://www.w3.org/2007/uwa/context/deliverycontext.owl#" xmlns:ex="http://www.example.org/2007/uwa/context/examples.owl#" xmlns:hard="http://www.w3.org/2007/uwa/context/hardware.owl#" xmlns:java="http://www.w3.org/2007/uwa/context/java.owl#" xmlns:loc="http://www.w3.org/2007/uwa/context/location.owl#" xmlns:net="http://www.w3.org/2007/uwa/context/network.owl#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:push="http://www.w3.org/2007/uwa/context/push.owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:soft="http://www.w3.org/2007/uwa/context/software.owl#" xmlns:web="http://www.w3.org/2007/uwa/context/web.owl#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xml:base="http://www.w3.org/2007/uwa/context/network.owl">
  <common:TermGroup rdf:about="#TermGroup_Network">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Network</rdfs:label>
        <common:termGroupPrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string">net</common:termGroupPrefix>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This group models classes, properties and instances that have to do with the Network characteristics of the Delivery Context</rdfs:comment>
        <common:normativeStatus rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Utility"/>
      </common:TermGroup>
  <owl:Class rdf:about="#NetworkSupport">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents the network characteristics of a device</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Context_NetworkEntity"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#supportedNetworkModes"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#NetworkMode"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#NetworkBearer"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#supportedNetworkBearers"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Network Support</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#NetworkBearer">
