      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#NetworkBearerTechnology"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#bearerTechnology"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Proxy">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Proxy</rdfs:label>
    <common:informativeReference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.w3.org/TR/di-gloss/#def-proxy</common:informativeReference>
