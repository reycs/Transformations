    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Present Networks</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#connectedNetworks">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Connected Networks</rdfs:label>
    <rdfs:domain rdf:resource="#NetworkSupport"/>
    <rdfs:range rdf:resource="#Network"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The networks to which a device is currently connected</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#proxy">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Proxy</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents one or more proxies that can be present while using a Network Bearer</rdfs:comment>
    <rdfs:domain rdf:resource="#NetworkBearer"/>
    <rdfs:range rdf:resource="#HttpProxy"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl#activeComponent"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
