    <rdfs:subClassOf rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl#Context_Entity"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Context Network Entity</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#availableNetworkBearers">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The available network bearers offered by a Network or available for a Device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Available Network Bearers</rdfs:label>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl#component"/>
    </rdfs:subPropertyOf>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#available"/>
    </rdfs:subPropertyOf>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#supportedNetworkBearers"/>
    </rdfs:subPropertyOf>
    <rdfs:range rdf:resource="#NetworkBearer"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Network"/>
          <owl:Class rdf:about="#NetworkSupport"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#supportedNetworkBearers">
    <common:relatedUAProfEntity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SupportedBearers</common:relatedUAProfEntity>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property holds the set of network bearers supported by a Network or by a Device.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Supported Network Bearers</rdfs:label>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Network"/>
          <owl:Class rdf:about="#NetworkSupport"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#NetworkBearer"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#supports"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#presentNetworks">
