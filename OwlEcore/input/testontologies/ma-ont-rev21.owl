    <!-- http://www.w3.org/ns/ma-ont#accessConditions -->

    <owl:DatatypeProperty rdf:about="#accessConditions">
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:subPropertyOf rdf:resource="#policy"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:comment
            >To define access conditions for the media resource</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#accessConditionsUri -->

    <owl:DatatypeProperty rdf:about="#accessConditionsUri">
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:subPropertyOf rdf:resource="#policyUri"/>
        <rdfs:domain rdf:resource="#MediaResource"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#alternativeTitle -->

    <owl:DatatypeProperty rdf:about="#alternativeTitle">
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:comment
            >To provide an alternative title</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#title"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#averageBitrate -->

    <owl:DatatypeProperty rdf:about="#averageBitrate">
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:comment
            >To provide the average bitrate at which the media resource can be played in kilobits/second</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#MediaResource"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="#Image"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#collectionName -->

    <owl:DatatypeProperty rdf:about="#collectionName">
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:comment
            >The name by which a collection (e.g. series) is known</rdfs:comment>
        <rdfs:domain rdf:resource="#Collection"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#copyright -->

    <owl:DatatypeProperty rdf:about="#copyright">
        <rdfs:comment
            >To express a copyright statement</rdfs:comment>
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#creationDate -->

    <owl:DatatypeProperty rdf:about="#creationDate">
        <rdfs:subPropertyOf rdf:resource="#date"/>
        <rdfs:comment
            >Date of creation of the media resource</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#date -->

    <owl:DatatypeProperty rdf:about="#date">
        <rdfs:comment
            >Any date related to the media resource. Dates of creation, recording, editing or release will be defined in sub-properties</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;date"/>
        <rdfs:domain rdf:resource="#MediaResource"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#description -->

    <owl:DatatypeProperty rdf:about="#description">
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:comment
            >A textual description of the media resource. This can be specialised by using sub-properties e.g. &apos;summary&apos; or &apos;script&apos;</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#duration -->

    <owl:DatatypeProperty rdf:about="#duration">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#AudioTrack"/>
                    <rdf:Description rdf:about="#VideoTrack"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:comment
            >To define the duration of the resource</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#editDate -->

    <owl:DatatypeProperty rdf:about="#editDate">
        <rdfs:comment
            >Date of editing of the media resource</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#date"/>
        <rdfs:range rdf:resource="&xsd;date"/>
        <rdfs:domain rdf:resource="#MediaResource"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#format -->

    <owl:DatatypeProperty rdf:about="#format">
        <rdfs:comment
            >The MIME type of the resource (e.g., wrapper or bucket media types), ideally including as much information as possible about the resource such as media type parameters, for example, using the &quot;codecs&quot; parameter [RFC 4281].</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain rdf:resource="#MediaResource"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#fragmentLocator -->

    <owl:DatatypeProperty rdf:about="#fragmentLocator">
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:subPropertyOf rdf:resource="#locator"/>
        <rdfs:domain rdf:resource="#MediaFragment"/>
        <rdfs:comment
            >The logical address at which a fragment of the resource can be accessed e.g. using a W3C MFWG fragment locator.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#fragmentName -->

    <owl:DatatypeProperty rdf:about="#fragmentName">
        <rdfs:comment
            >The name given to a media fragment</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain rdf:resource="#MediaFragment"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#frameHeight -->

    <owl:DatatypeProperty rdf:about="#frameHeight">
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:comment
            >The number o frame size units to measure the frame height</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#VideoTrack"/>
                    <rdf:Description rdf:about="#Image"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#frameRate -->

    <owl:DatatypeProperty rdf:about="#frameRate">
        <rdfs:domain rdf:resource="#VideoTrack"/>
        <rdfs:comment
            >The frame rate of the video signal in frame per second</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#frameSizeUnit -->

    <owl:DatatypeProperty rdf:about="#frameSizeUnit">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#VideoTrack"/>
                    <rdf:Description rdf:about="#Image"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:comment
            >The unit used to express the frame width or height. The unit by default is &apos;pixel&apos;</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#frameWidth -->

    <owl:DatatypeProperty rdf:about="#frameWidth">
        <rdfs:comment
            >The number o frame size units to measure the frame width</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#VideoTrack"/>
                    <rdf:Description rdf:about="#Image"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#hasRatingValue -->

    <owl:DatatypeProperty rdf:about="#hasRatingValue">
        <rdfs:range rdf:resource="&xsd;float"/>
        <rdfs:comment
            >To express a rating value defined in a rating classification scheme</rdfs:comment>
        <rdfs:domain rdf:resource="#Rating"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#identifier -->

    <owl:DatatypeProperty rdf:about="#identifier">
        <rdfs:comment
            >This property is intended to provide an alternative identifier for the
 resource, which has no URI counterpart.

 In the case where the alternative identifier can be expressed as a
 URI, it is recommended to use owl:sameAs instead.</rdfs:comment>
        <rdfs:domain rdf:resource="#MediaResource"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#locationAltitude -->

    <owl:DatatypeProperty rdf:about="#locationAltitude">
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:comment
            >To define the altitude of a location in meters</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#locationCoordinateSystem -->

    <owl:DatatypeProperty rdf:about="#locationCoordinateSystem">
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:comment
            >The system used to express the longitude and latitude of a location</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#locationLatitude -->

    <owl:DatatypeProperty rdf:about="#locationLatitude">
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:comment
            >The latitude of the location</rdfs:comment>
        <rdfs:domain rdf:resource="#Location"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#locationLongitude -->

    <owl:DatatypeProperty rdf:about="#locationLongitude">
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:comment
            >To define the longitude of a location</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#locationName -->

    <owl:DatatypeProperty rdf:about="#locationName">
        <rdfs:comment
            >The name by a location is known</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain rdf:resource="#Location"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#locator -->

    <owl:DatatypeProperty rdf:about="#locator">
        <rdfs:comment
            >The logical address at which the resource can be accessed (e.g. a URL, or a DVB URI).</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:domain rdf:resource="#MediaResource"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#mainOriginalTitle -->

    <owl:DatatypeProperty rdf:about="#mainOriginalTitle">
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:subPropertyOf rdf:resource="#title"/>
        <rdfs:comment
            >To provide the  mina title in its original version / language</rdfs:comment>
        <rdfs:domain rdf:resource="#MediaResource"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#numberOfAudioTracks -->

    <owl:DatatypeProperty rdf:about="#numberOfAudioTracks">
        <rdfs:subPropertyOf rdf:resource="#numberOfTracks"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:comment
            >The number of audio  tracks composing the media resource.</rdfs:comment>
        <rdfs:domain rdf:resource="#MediaResource"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#numberOfTracks -->

    <owl:DatatypeProperty rdf:about="#numberOfTracks">
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:domain rdf:resource="#MediaFragment"/>
        <rdfs:comment
            >The number of audio and video tracks composing the media resource.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#numberOfVideoTracks -->

    <owl:DatatypeProperty rdf:about="#numberOfVideoTracks">
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:comment
            >The number of video tracks composing the media resource.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:subPropertyOf rdf:resource="#numberOfTracks"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#permissions -->

    <owl:DatatypeProperty rdf:about="#permissions">
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:comment
            >To describe usage permissions for the media resource</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="#policy"/>
        <rdfs:domain rdf:resource="#MediaResource"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#permissionsUri -->

    <owl:DatatypeProperty rdf:about="#permissionsUri">
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:subPropertyOf rdf:resource="#policyUri"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#policy -->

    <owl:DatatypeProperty rdf:about="#policy">
        <rdfs:comment
            >To epxress a usage policy for the media resource</rdfs:comment>
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#policyUri -->

    <owl:DatatypeProperty rdf:about="#policyUri">
        <rdfs:comment
            >To provide a uri to the location of an instance of a policy</rdfs:comment>
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#ratingScaleMax -->

    <owl:DatatypeProperty rdf:about="#ratingScaleMax">
        <rdfs:comment
            >The maximum value of the scale used for rating a media resource</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;float"/>
        <rdfs:domain rdf:resource="#Rating"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#ratingScaleMin -->

    <owl:DatatypeProperty rdf:about="#ratingScaleMin">
        <rdfs:range rdf:resource="&xsd;float"/>
        <rdfs:comment
            >The minimum value of the scale used for rating a media resource</rdfs:comment>
        <rdfs:domain rdf:resource="#Rating"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#recordDate -->

    <owl:DatatypeProperty rdf:about="#recordDate">
        <rdfs:subPropertyOf rdf:resource="#date"/>
        <rdfs:range rdf:resource="&xsd;date"/>
        <rdfs:comment
            >Date of recording of the me dia resource</rdfs:comment>
        <rdfs:domain rdf:resource="#MediaResource"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#releaseDate -->

    <owl:DatatypeProperty rdf:about="#releaseDate">
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:range rdf:resource="&xsd;date"/>
        <rdfs:subPropertyOf rdf:resource="#date"/>
        <rdfs:comment
            >Date of release of the media resource</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#samplingRate -->

    <owl:DatatypeProperty rdf:about="#samplingRate">
        <rdfs:domain rdf:resource="#AudioTrack"/>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:comment
            >The sample rate of the audio signal in samples per second</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#size -->

    <owl:DatatypeProperty rdf:about="#size">
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:range rdf:resource="&xsd;long"/>
        <rdfs:comment
            >The size of the media resource file in bytes</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#title -->

    <owl:DatatypeProperty rdf:about="#title">
        <rdfs:domain rdf:resource="#MediaResource"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:comment
            >Specifies the title or name given to the resource. The type can be used as a sub-property to optionally refine the category of the title.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
    


    <!-- http://www.w3.org/ns/ma-ont#Actor -->

    <owl:Class rdf:about="#Actor">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#actorIs"/>
                <owl:someValuesFrom rdf:resource="#Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="#Contributor"/>
        <rdfs:comment
            >An agent acting in the media resource</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#Agent -->

    <owl:Class rdf:about="#Agent">
        <owl:disjointWith rdf:resource="#TargetAudience"/>
        <owl:disjointWith rdf:resource="#Location"/>
        <owl:disjointWith rdf:resource="#Contributor"/>
        <owl:disjointWith rdf:resource="#Rating"/>
        <rdfs:comment
            >A person or organisation contributing to the media resource</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#AudioTrack -->

    <owl:Class rdf:about="#AudioTrack">
        <rdfs:subClassOf rdf:resource="#MediaFragment"/>
        <owl:disjointWith rdf:resource="#VideoTrack"/>
        <rdfs:comment
            >One or more audio tracks composing the media resource</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#Collection -->

    <owl:Class rdf:about="#Collection">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasMember"/>
                <owl:someValuesFrom rdf:resource="#MediaResource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#MediaResource"/>
        <owl:disjointWith rdf:resource="#TargetAudience"/>
        <owl:disjointWith rdf:resource="#Location"/>
        <owl:disjointWith rdf:resource="#Rating"/>
        <owl:disjointWith rdf:resource="#Contributor"/>
        <owl:disjointWith rdf:resource="#Agent"/>
        <rdfs:comment
            >Any group of media resource e.g. a series</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#Contributor -->

    <owl:Class rdf:about="#Contributor">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#contributorIs"/>
                <owl:someValuesFrom rdf:resource="#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#TargetAudience"/>
        <owl:disjointWith rdf:resource="#Rating"/>
        <rdfs:comment
            >An agent having contributed to the media resource</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#CopyrightHolder -->

    <owl:Class rdf:about="#CopyrightHolder">
        <rdfs:subClassOf rdf:resource="#Contributor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#copyrightHolderIs"/>
                <owl:someValuesFrom rdf:resource="#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >An agent holding right on the media resource</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#Creator -->

    <owl:Class rdf:about="#Creator">
        <rdfs:subClassOf rdf:resource="#Contributor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#creatorIs"/>
                <owl:someValuesFrom rdf:resource="#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >The creator / author of the media resource</rdfs:comment>
        <rdfs:comment
            >An agent in charge of publishing the media resource</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#Image -->

    <owl:Class rdf:about="#Image">
        <rdfs:subClassOf rdf:resource="#MediaResource"/>
        <owl:disjointWith rdf:resource="#AudioTrack"/>
        <rdfs:comment
            >A still image / thumbnail / key fra,e related to the media resource or being the media resource itself</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#Location -->

    <owl:Class rdf:about="#Location">
        <owl:disjointWith rdf:resource="#Contributor"/>
        <owl:disjointWith rdf:resource="#TargetAudience"/>
        <owl:disjointWith rdf:resource="#Rating"/>
        <rdfs:comment
            >A location related to the media resource, e.g. depicted in the resource (possibly fictional) or where the resource was created (shooting location), etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#MediaFragment -->

    <owl:Class rdf:about="#MediaFragment">
        <rdfs:subClassOf rdf:resource="#MediaResource"/>
        <rdfs:comment
            >One of more media fragment (audio and/or video) composing an audiovisual media resource</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#MediaResource -->

    <owl:Class rdf:about="#MediaResource">
        <owl:disjointWith rdf:resource="#Rating"/>
        <owl:disjointWith rdf:resource="#Agent"/>
        <owl:disjointWith rdf:resource="#Location"/>
        <owl:disjointWith rdf:resource="#Contributor"/>
        <owl:disjointWith rdf:resource="#TargetAudience"/>
        <rdfs:comment
            >Am image or an audiovisual media resource, which can be compsoed of at least one audio and/ or video fragment</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#Organisation -->

    <owl:Class rdf:about="#Organisation">
        <rdfs:subClassOf rdf:resource="#Agent"/>
        <owl:disjointWith rdf:resource="#Person"/>
        <rdfs:comment
            >An organisation or moral agent</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#Person -->

    <owl:Class rdf:about="#Person">
        <rdfs:subClassOf rdf:resource="#Agent"/>
        <rdfs:comment
            >A physical person</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#Publisher -->

    <owl:Class rdf:about="#Publisher">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#publisherIs"/>
                <owl:someValuesFrom rdf:resource="#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="#Contributor"/>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#Rating -->

    <owl:Class rdf:about="#Rating">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:comment
            >All the information about the rating given to a media resource</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#TargetAudience -->

    <owl:Class rdf:about="#TargetAudience">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <owl:disjointWith rdf:resource="#Rating"/>
        <rdfs:comment
            >The target audience for which the media resource is intended</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/ma-ont#VideoTrack -->

    <owl:Class rdf:about="#VideoTrack">
        <rdfs:subClassOf rdf:resource="#MediaFragment"/>
        <rdfs:comment
            >One or more video tracks composing the media resource</rdfs:comment>
    </owl:Class>
</rdf:RDF>
