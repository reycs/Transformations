<?xml version="1.0" encoding="UTF-8"?><rdf:RDF xmlns="http://www.w3.org/2007/uwa/context/hardware.owl#" xmlns:assert="http://www.owl-ontologies.com/assert.owl#" xmlns:common="http://www.w3.org/2007/uwa/context/common.owl#" xmlns:dc="http://purl.org/dc/terms/" xmlns:dcn="http://www.w3.org/2007/uwa/context/deliverycontext.owl#" xmlns:ex="http://www.example.org/2007/uwa/context/examples.owl#" xmlns:hard="http://www.w3.org/2007/uwa/context/hardware.owl#" xmlns:java="http://www.w3.org/2007/uwa/context/java.owl#" xmlns:loc="http://www.w3.org/2007/uwa/context/location.owl#" xmlns:net="http://www.w3.org/2007/uwa/context/network.owl#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:push="http://www.w3.org/2007/uwa/context/push.owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:soft="http://www.w3.org/2007/uwa/context/software.owl#" xmlns:web="http://www.w3.org/2007/uwa/context/web.owl#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xml:base="http://www.w3.org/2007/uwa/context/hardware.owl">
  <common:TermGroup rdf:about="#TermGroup_Hardware">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A group of terms with properties, classes and instances that model the hardware elements of a Delivery Context</rdfs:comment>
        <common:termGroupPrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hard</common:termGroupPrefix>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hardware</rdfs:label>
        <common:normativeStatus rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#Utility"/>
      </common:TermGroup>
  <owl:Class rdf:about="#GraphicDisplay">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a graphic display</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Display"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Graphic Display</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Context_HardwareEntity">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is the base class for those ontology entities that have to do with hardware</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Context Hardware Entity</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl#Context_Entity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#HardwareStatus">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents the status of a hardware component</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Context_HardwareEntity"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hardware Status</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Keyboard">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a keyboard in a device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Keyboard</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#InputDevice"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#InfraredHardware">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents the infrared hardware on a device</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#HardwareComponent"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">InfraredHardware</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Camera">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#InputDevice"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Camera</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a camera in a device</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Battery">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#HardwareComponent"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Battery</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a battery in a device</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#BluetoothProfile">
    <common:relatedUAProfEntity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">BluetoothProfile</common:relatedUAProfEntity>
    <rdfs:subClassOf rdf:resource="#Context_HardwareEntity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#name"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#id"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bluetooth Profile</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class describes a specific Bluetooth profile used to support a particular user scenario</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Microphone">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a microphone in a device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Microphone</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#InputDevice"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#OutputDevice">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents an output mechanism that can be available in a device</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#HardwareComponent"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Output Device</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#DeviceHardware">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Device Hardware</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:maxCardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#primaryCamera"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#InputDevice"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#inputDevice"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#pointingResolution"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="#PointingResolution"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#HardwareComponent"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents the hardware in a device</rdfs:comment>
    <common:relatedUAProfEntity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HardwarePlatform</common:relatedUAProfEntity>
  </owl:Class>
  <owl:Class rdf:about="#BluetoothStatus">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents the possible statuses of Bluetooth in a device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bluetooth Status</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#id"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Context_HardwareEntity"/>
  </owl:Class>
  <owl:Class rdf:about="#Display">
    <common:relatedUAProfEntity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HardwarePlatform</common:relatedUAProfEntity>
    <rdfs:subClassOf rdf:resource="#OutputDevice"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Display</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a visual display on a device</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#MemoryCard">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a storage card which can be attached to a device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Memory Card</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#volatile"/>
        </owl:onProperty>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#MemoryUnit"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#HardwareComponent">
    <rdfs:subClassOf rdf:resource="#Context_HardwareEntity"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2007/uwa/context/deliverycontext.owl#Aspect"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#status"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#HardwareStatus"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hardware Component</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a hardware component of a Device</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#WiFiHardware">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents  hardware in a device that can be used to access to WiFi networks</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#HardwareComponent"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WiFi Hardware</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#PointingResolution">
    <common:relatedUAProfEntity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PointingResolution</common:relatedUAProfEntity>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pointing Resolution</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents the precision with which the pointing can be achieved on a  device</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#id"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Context_HardwareEntity"/>
  </owl:Class>
  <owl:Class rdf:about="#MemoryUnit">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#size"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#HardwareComponent"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Memory Unit</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents some memory used in a device. It might, for example, be built-in memory that is part of the device, or it might be removable memory on some form of storage card</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#CPU">
    <rdfs:subClassOf rdf:resource="#HardwareComponent"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CPU</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class describes a central processing unit associated with a device</rdfs:comment>
    <common:relatedUAProfEntity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CPU</common:relatedUAProfEntity>
  </owl:Class>
  <owl:Class rdf:about="#InputDevice">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents an input mechanism that can be available in a device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Input Device</rdfs:label>
    <rdfs:subClassOf rdf:resource="#HardwareComponent"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:maxCardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#id"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#TactileDisplay">
    <rdfs:subClassOf rdf:resource="#Display"/>
    <rdfs:subClassOf rdf:resource="#InputDevice"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tactile Display</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A display that is tac-sensitive, allowing users to interact directly with the objects shown on the screen</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#CellularHardware">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cellular Hardware</rdfs:label>
    <rdfs:subClassOf rdf:resource="#HardwareComponent"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents device hardware that can be used to access to mobile operator telephony networks</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Speaker">
    <rdfs:subClassOf rdf:resource="#OutputDevice"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Speaker</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a speaker component in a device</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#NfcHardware">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NFC Hardware</rdfs:label>
    <rdfs:subClassOf rdf:resource="#HardwareComponent"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents the NFC hardware on a device</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#BluetoothHardware">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bluetooth Hardware</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#BluetoothProfile"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#supportedBluetoothProfiles"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#HardwareComponent"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents device hardware that can be used to access to Bluetooth technology</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#CharacterDisplay">
    <rdfs:subClassOf rdf:resource="#Display"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Character Display</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class represents a character oriented display</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#battery">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hardwareComponent"/>
    </rdfs:subPropertyOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Battery</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents one or more batteries in a device</rdfs:comment>
    <rdfs:domain rdf:resource="#DeviceHardware"/>
    <rdfs:range rdf:resource="#Battery"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#secondaryCamera">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Secondary Camera</rdfs:label>
    <rdfs:domain rdf:resource="#DeviceHardware"/>
    <rdfs:range rdf:resource="#Camera"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hardwareComponent"/>
    </rdfs:subPropertyOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents a secondary camera of a device</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#outputCharacterSets">
    <rdfs:domain rdf:resource="#DeviceHardware"/>
    <rdfs:range rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#CharacterSet"/>
    <common:relatedUAProfEntity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OutputCharSet</common:relatedUAProfEntity>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#supports"/>
    </rdfs:subPropertyOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property defines the character sets supported by the device for output</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Output Character Sets</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#display">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents a display associated with a device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Display</rdfs:label>
    <rdfs:domain rdf:resource="#DeviceHardware"/>
    <rdfs:range rdf:resource="#Display"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hardwareComponent"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#primary">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents a relationship between the device hardware and the primary components it contains</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Primary</rdfs:label>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hardwareComponent"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#memory">
    <rdfs:range rdf:resource="#MemoryUnit"/>
    <rdfs:domain rdf:resource="#DeviceHardware"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#hardwareComponent"/>
    </rdfs:subPropertyOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents the hardware memory component/s of a device</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Memory</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#supportedBluetoothProfiles">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property defines the Bluetooth profiles supported by the device</rdfs:comment>
    <common:relatedUAProfEntity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">BluetoothProfile</common:relatedUAProfEntity>
    <rdfs:range rdf:resource="#BluetoothProfile"/>
    <rdfs:domain rdf:resource="#BluetoothHardware"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Supported Bluetooth Profiles</rdfs:label>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/common.owl#supports"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hardwareComponent">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hardware Component</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property represents a hardware component</rdfs:comment>
    <rdfs:domain rdf:resource="#DeviceHardware"/>
    <rdfs:range rdf:resource="#HardwareComponent"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.w3.org/2007/uwa/context/deliverycontext.owl#component"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#inputDevice">
    <rdfs:domain rdf:resource="#DeviceHardware"/>
    <rdfs:range rdf:resource="#InputDevice"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The input mechanisms supported by a device</rdfs:comment>
    <common:relatedUAProfEntity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Keyboard</common:relatedUAProfEntity>
    <common:relatedDDRCoreVoc rdf:datatype="http://www.w3.org/2001/XMLSchema#string">inputDevices</common:relatedDDRCoreVoc>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#supports"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Input Devices</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#extensionMemory">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property identifies additional memory that is provided to a device, typically in the form of removable memory cards</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Extension Memory</rdfs:label>
    <rdfs:domain rdf:resource="#DeviceHardware"/>
    <rdfs:range rdf:resource="#MemoryUnit"/>
    <rdfs:subPropertyOf rdf:resource="#memory"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#inputCharacterSets">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property defines the character sets supported by the device for input</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#supports"/>
    <rdfs:domain rdf:resource="#DeviceHardware"/>
    <rdfs:range rdf:resource="http://www.w3.org/2007/uwa/context/common.owl#CharacterSet"/>
    <common:relatedUAProfEntity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">InputCharSet</common:relatedUAProfEntity>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Input Character Sets</rdfs:label>
